import java.util.Scanner;
public class uri1098{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    double a1 =0;
    double k1 =1;
    double l1 =2;
    double m1 =3;
    for(int i=0; a1<=2.1f; i++){
      System.out.println("I="+a1+" J="+k1);
      System.out.println("I="+a1+" J="+l1);
      System.out.println("I="+a1+" J="+m1);
      a1=a1+.2f;
      k1=k1+.2f;
      l1=l1+.2f;
      m1=m1+.2f;    
    }
  }
}