import java.util.Scanner;
public class 1164{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    for(int i=0; i<n; i++){
      int sum=0;
      int number = sc.nextInt();
      if(number%i==0){
        sum+=i;
      }
      if(sum==number){
        System.out.println(number+" eh perfeito");
      }
      else if(sum!=number){
        System.out.println(number+" nao eh perfeito");
      }
    }
  }
}