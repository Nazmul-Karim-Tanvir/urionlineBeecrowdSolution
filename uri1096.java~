import java.util.Scanner;
public class uri1096{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a =1;
    int k =7;
    int l =6;
    int m =5;
    for(int i=1; i<=5; i++){
      System.out.println("I"+a+" J="+k);
      System.out.println("I"+a+" J="+l);
      System.out.println("I"+a+" J="+m);
      a+=2;
    } 
  }
}