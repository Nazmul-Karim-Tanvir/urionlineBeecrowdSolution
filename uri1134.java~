import java.util.Scanner;
public class uri1134{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int alcohol =0;
    int gasoline =0;
    int diesel =0;
    if(n>=1 && <=4){
      while(n==4){
        if(n==1){
          alcohol++;
        }
        if(n==2){
          gasoline++;
        }
        if(n==3){
          diesel++;
        }
      }
    }
    else{
    
    }
  }
}